{
  "name": "vector",
  "version": "0.4.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:pages": "ng build --prod --output-path docs --base-href=/vector/",
    "build:prod": "ng build --prod",
    "deploy": "npm run build:pages && cp docs/index.html docs/404.html && gh-pages -d docs",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "lint:watch": "nodemon --exec \"ng lint\" --ext ts --ignore node_modules/",
    "e2e": "ng e2e",
    "preversion": "ng lint && ng test -- --no-watch --no-progress",
    "postversion": "git push && git push --tags"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.11",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "^9.1.11",
    "@angular/compiler": "^9.1.11",
    "@angular/core": "^9.1.11",
    "@angular/flex-layout": "^8.0.0-beta.27",
    "@angular/forms": "^9.1.11",
    "@angular/material": "^8.2.3",
    "@angular/material-moment-adapter": "^8.2.3",
    "@angular/platform-browser": "^9.1.11",
    "@angular/platform-browser-dynamic": "^9.1.11",
    "@angular/router": "^9.1.11",
    "core-js": "^2.6.11",
    "hammerjs": "^2.0.8",
    "lasp-base-app-snippets": "^1.1.0",
    "lasp-footer": "^0.5.0",
    "lasp-nav": "^1.2.3",
    "lasp-search": "^0.1.0",
    "moment": "^2.24.0",
    "ngx-markdown": "^8.2.2",
    "rxjs": "^6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.0",
    "@angular/cli": "^9.1.9",
    "@angular/compiler-cli": "^9.1.11",
    "@angular/language-service": "^9.1.11",
    "@types/jasmine": "^2.8.16",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^12.11.1",
    "ajv": "^6.12.0",
    "codelyzer": "^5.1.2",
    "gh-pages": "^2.2.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "^5.4.3",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "tslint-default": "^1.0.2",
    "typescript": "~3.8.3"
  }
}
